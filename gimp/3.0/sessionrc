                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-brush-list"
                (tab-style preview)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-image-list"
                (tab-style icon)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-document-list"
                (tab-style icon)
                (aux-info
                    (show-button-bar "true"))))
        (book
            (position 414)
            (current-page 0)
            (dockable "gimp-layer-list"
                (tab-style icon)
                (preview-size 32)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-dynamics-list"
                (tab-style preview)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-dynamics-editor"
                (tab-style icon)
                (aux-info
                    (edit-active "true")
                    (current-data "Eraser"))))))
